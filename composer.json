{
  "name": "rivieratravel/jenkins-api",
  "type": "library",
  "description": "Jenkins PHP API",
  "keywords": ["jenkins", "api"],
  "homepage": "https://github.com/RivieraTravel/jenkins-php-api",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "JenkinsKhan\\": "src/"
    }
  },
  "require": {
      "php": ">=5.6",
      "ext-curl": "*",
      "ext-json": "*",
      "ext-mbstring": "*",
      "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "atoum/atoum": "2.0.1"
  },
  "config": {
    "bin-dir": "bin"
  },
  "authors": [
    {
      "name": "jenkins-khan",
      "homepage": "https://github.com/jenkins-khan/jenkins-php-api"
    },
    {
      "name": "RivieraTravel",
      "homepage": "https://github.com/RivieraTravel"
    }
  ]
}
